{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generar-convenio",
      "dest": "/api/generar-convenio.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
